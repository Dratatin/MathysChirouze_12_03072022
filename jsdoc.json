{
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "template": "node_modules/postman-jsdoc-theme",
        "destination": "./docs/",
        "recurse": true,
        "readme": "./readme.md"
    }
}